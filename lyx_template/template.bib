	pages = {253--257}
}

@article{bernanke_agency_1989,
	title = {Agency costs, net worth, and business fluctuations},
	volume = {79},
	issn = {0002-8282},
	abstract = {A simple neoclassical model of intrinsic business cycle dynamics is developed in which borrowers' balance sheet positions play an important role. The critical insight obtained is that the agency costs of undertaking physical investments are inversely related to the entrepreneur's/borrower's net worth. As a result, accelerator effects on investment may emerge. Strengthened borrower balance sheets resulting from good times expand investment demand, which in turn tends to amplify the upturn. On the other hand, weakened balance sheets in bad times do just the opposite. The aggregate effects of productivity shocks may be asymmetric, since the agency problem may only bind on the downturn side. Further, redistributions or other shocks that affect borrowers' balance sheets (as may occur in the case of debt-deflation) will have aggregate real effects. While the competitive solution of the model economy is not guaranteed to be Pareto optimal, it is efficient in a limited, intra-generational sense.},
	number = {1},
	journal = {American Economic Review},
	author = {Bernanke, Ben and Gertler, Mark},
