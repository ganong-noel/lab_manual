@incollection{campbell1989consumption,
	title = {Consumption, {Income} and {Interest} {Rates}: {Reinterpreting} the {Time} {Series} {Evidence}},
	shorttitle = {Consumption, {Income} and {Interest} {Rates}},
	url = {https://ideas.repec.org/h/nbr/nberch/10965.html},
	abstract = {This paper proposes that the time-series data on consumption, income, and interest rates are best viewed as generated not by a single representative consumer but by two groups of consumers. Half the consumers are forward-looking and consume their permanent income, but are extremely reluctant to substitute consumption temporarily. Half the consumers follow the "rule of thumb" of consuming their current income. The paper documents three empirical regularities that, it argues, are best explained by this medal. First, expected changes in income are associated with expected changes in consumption. Second, expected real interest rates are not associated with expected changes in consumption. Third, periods in which consumption is high relative to income are typically followed by high growth in income. The paper concludes by briefly discussing the implications of these findings for economic policy and economic research.{\textless}br{\textgreater}{\textless}small{\textgreater}(This abstract was borrowed from another version of this item.){\textless}/small{\textgreater}},
	language = {en},
	urldate = {2018-10-24},
	booktitle = {{NBER} {Chapters}},
