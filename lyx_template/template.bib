@article{lorenzoni_inefficient_2008,
	title = {Inefficient {Credit} {Booms}},
	volume = {75},
	issn = {0034-6527},
	doi = {10.1111/j.1467-937X.2008.00494.x},
	abstract = {This paper studies the welfare properties of competitive equilibria in an economy with financial frictions hit by aggregate shocks. In particular, it shows that competitive financial contracts can result in excessive borrowing and excessive volatility . Even though from a first‐best perspective the equilibrium always displays under‐borrowing, from a second‐best point of view excessive borrowing can arise. The inefficiency is due to the combination of limited commitment in financial contracts and the fact that asset prices are determined in a spot market. This generates a pecuniary externality that is not internalized in private contracts. The model provides a framework to evaluate preventive policies, which can be used during a credit boom to reduce the expected costs of a financial crisis.},
